{
  "name": "xml-flow",
  "version": "1.0.4",
  "description": "An XML/HTML stream reader, now with less suck.",
  "main": "lib/xml-flow.js",
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "mocha test/*.js",
    "coverage": "nyc npm run test",
    "travis": "npm run lint && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewmatician/xml-flow.git"
  },
  "keywords": [
    "xml",
    "stream",
    "parse",
    "huge files"
  ],
  "author": "Matthew Larson <matthewmatician@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewmatician/xml-flow/issues"
  },
  "homepage": "https://github.com/matthewmatician/xml-flow",
  "dependencies": {
    "sax": "^1.2.4"
  },
  "devDependencies": {
    "chai": "*",
    "coveralls": "*",
    "eslint": "^6.0.0",
    "eslint-plugin-mocha": "^5.3.0",
    "extend": ">=3.0.2",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "all": true,
    "reporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "include": "lib/**/*.js",
    "exclude": "test/**/*.js"
  }
